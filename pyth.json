      "def dlss(node, goal, depth, path, tree):",
      "    if node == goal:",
      "        path.append(goal)",
      "        return True",
      "    if depth == 0 or node not in tree:",
      "        return False",
      "    for child in tree[node]:",
      "        if dlss(child, goal, depth - 1, path, tree):",
      "            path.append(node)",
      "            return True",
      "    return False",
      "",
      "def iterative_deepening(start, goal, max_depth, tree):",
